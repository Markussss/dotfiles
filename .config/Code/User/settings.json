{
    "editor.fontSize": 16,
    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingOvertype": "always",
    "editor.copyWithSyntaxHighlighting": false,
    "editor.lineHeight": 26,
    "editor.renderLineHighlight": "all",
    "editor.selectionClipboard": false,
    "editor.showFoldingControls": "always",
    "editor.tabCompletion": "on",
    "editor.tabSize": 2,
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 100,
    "editor.wrappingIndent": "indent",
    "editor.cursorSurroundingLines": 5,
    "editor.cursorStyle": "line-thin",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.quickSuggestions": true,
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.quickSuggestionsDelay": 0,
    "editor.suggest.maxVisibleSuggestions": 15,
    "files.autoGuessEncoding": true,
    "files.defaultLanguage": "markdown",
    "files.hotExit": "onExitAndWindowClose",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "workbench.enableExperiments": false,
    "workbench.sideBar.location": "right",
    "workbench.tree.renderIndentGuides": "always",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "zenMode.fullScreen": false,
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "toggle",
    "window.titleBarStyle": "custom",
    "explorer.incrementalNaming": "smart",
    "explorer.openEditors.visible": 9001,
    "workbench.tree.indent": 32,
    "search.exclude": {
        "**/*.min.js": true
    },
    "files.exclude": {
        "**/node_modules/**": true,
        "**/vendor/**": true
    },
    "search.searchOnTypeDebouncePeriod": 0,
    "search.showLineNumbers": true,
    "search.useIgnoreFiles": false,
    "debug.console.fontSize": 16,
    "debug.console.lineHeight": 26,
    "scm.alwaysShowProviders": true,
    "scm.alwaysShowActions": true,
    "scm.countBadge": "off",
    "terminal.explorerKind": "external",
    "terminal.external.linuxExec": "gnome-terminal",
    "terminal.external.osxExec": "iTerm2.app",
    "problems.showCurrentInStatus": true,
    "comments.openPanel": "neverOpen",
    "update.mode": "manual",
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "php.suggest.basic": false,
    "php.validate.enable": false,
    "editor.wordSeparators": "`~!@#$%^&*()_-=+[{]}\\\\|;:'\\\",.<>/?",
    "workbench.colorTheme": "Huacat Pink Theme"
}