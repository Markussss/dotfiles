{
    "editor.fontFamily": "Fira Code",
    "editor.fontSize": 18,
    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingOvertype": "always",
    "editor.cursorBlinking": "blink",
    "files.trimTrailingWhitespace": true,
    "search.useIgnoreFiles": true,
    "merge-conflict.codeLens.enabled": true,
    "merge-conflict.decorators.enabled": true,
    "editor.rulers": [
        100
    ],
    "editor.copyWithSyntaxHighlighting": false,
    "editor.lineHeight": 26,
    "editor.renderLineHighlight": "all",
    "editor.selectionClipboard": false,
    "editor.showFoldingControls": "always",
    "editor.tabCompletion": "on",
    "editor.cursorSurroundingLines": 3,
    "editor.cursorStyle": "line-thin",
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.minimap.enabled": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on",
        "other": "on"
    },
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.quickSuggestionsDelay": 0,
    "files.autoGuessEncoding": true,
    "files.defaultLanguage": "markdown",
    "files.hotExit": "onExitAndWindowClose",
    "files.trimFinalNewlines": true,
    "workbench.enableExperiments": false,
    "workbench.sideBar.location": "right",
    "workbench.tree.renderIndentGuides": "always",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "zenMode.fullScreen": false,
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "toggle",
    "window.titleBarStyle": "custom",
    "explorer.incrementalNaming": "smart",
    "explorer.openEditors.visible": 9001,
    "workbench.tree.indent": 32,
    "search.exclude": {
        "**/*.min.js": true
    },
    "search.searchOnTypeDebouncePeriod": 0,
    "search.showLineNumbers": true,
    "debug.console.lineHeight": 26,
    "scm.alwaysShowActions": true,
    "scm.countBadge": "off",
    "terminal.explorerKind": "external",
    "terminal.external.linuxExec": "gnome-terminal",
    "problems.showCurrentInStatus": true,
    "comments.openPanel": "neverOpen",
    "update.mode": "manual",
    "telemetry.telemetryLevel": "off",
    "telemetry.enableTelemetry": false, // deprecated in favor of "telemetry.telemetryLevel": "off", but let's keep them turned to false, just in case they decide to do a switcheroo
    "telemetry.enableCrashReporter": false, // deprecated in favor of "telemetry.telemetryLevel": "off", but let's keep them turned to false, just in case they decide to do a switcheroo
    "php.suggest.basic": false,
    "php.validate.enable": false,
    "editor.wordSeparators": "`~!@#$%^&*()_-=+[{]}\\\\|;:'\\\",.<>/?",
    "workbench.colorCustomizations": {
        // "editor.background": "#fff2ff"
    },
    "php-docblocker.returnGap": true,
    "php-docblocker.qualifyClassNames": true,
    "files.associations": {
        "*.php":"php"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue"
      ],
    "js/ts.implicitProjectConfig.checkJs": false,
    "namespaceResolver.autoSort": false,
    "namespaceResolver.sortOnSave": false,
    "namespaceResolver.sortAlphabetically": true,
    "workbench.editor.decorations.badges": false,
    "workbench.editor.decorations.colors": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "[php]": {
        "gitlens.codeLens.scopes": [
            "document"
        ],
        "editor.tabSize": 4
    },
    "[vue]": {
        "gitlens.codeLens.scopes": [
            "document"
        ],
        "editor.tabSize": 2
    },
    "[js]": {
        "gitlens.codeLens.scopes": [
            "document"
        ],
        "editor.tabSize": 2
    },
    "editor.tabSize": 2,
    "twig-language-2.tabSize": 4,
    "redhat.telemetry.enabled": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "php.executablePath": "/usr/bin/php",
    "codestream.serverUrl": "https://api.codestream.com",
    "vetur.experimental.templateInterpolationService": true,
    "vetur.format.defaultFormatter.css": "none",
    "vetur.format.defaultFormatter.js": "prettier-eslint",
    "vetur.format.defaultFormatter.less": "none",
    "vetur.format.defaultFormatter.postcss": "none",
    "vetur.format.defaultFormatter.pug": "none",
    "vetur.format.defaultFormatter.sass": "none",
    "vetur.format.defaultFormatter.scss": "none",
    "vetur.format.defaultFormatter.stylus": "none",
    "vetur.format.defaultFormatter.ts": "none",
    "vetur.underline.refValue": false,
    "vetur.validation.style": false,
    "vetur.validation.templateProps": true,
    "workbench.colorTheme": "Default Light+",
    "editor.inlineSuggest.enabled": true,
    "git.mergeEditor": false,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": false,
        "markdown": false
    }
}
